#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    int n;
    int px, py;
    int qx, qy;
    int rx, ry;
    cin >> n;
    while(n--)
    {
        cin >> px >> py >> qx >> qy;
        rx = qx + (qx - px);
        ry = qy + (qy - py);
        cout << rx << ' ' << ry << endl;
    }
    return 0;
}
