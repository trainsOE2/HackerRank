#include <iostream> 
typedef long long int lli;
using namespace std;


int main() {
 
    int arr_size;
    lli sum = 0;
    lli x;
    cin >> arr_size;
    lli arr[arr_size];
    for( int i=0 ; i<arr_size ; i++)
    {
        cin >> arr[i];
        sum += arr[i];
    }
    
    
    cout << sum;
        
    return 0;
}
